@echo off
setlocal enabledelayedexpansion

:: Check if running as administrator
net session >nul 2>&1
if %errorlevel% == 0 (
    echo Running as administrator...
    goto admin
) else (
    echo Please run as administrator...
    pause
    exit
)

:admin
set "sv_cheats=1"
echo Setting sv_cheats to %sv_cheats%

:: Close and relaunch TF2
taskkill /im tf.exe /f
start "tf.exe"

pause
